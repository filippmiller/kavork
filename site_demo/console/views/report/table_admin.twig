<table border="0" cellpadding="0" cellspacing="0" width="{{ width }}" style="background:#ffffff;border-collapse:collapse;border-spacing:0;color:#3d454c;font:16px / 24px 'Open Sans', 'helvetica' , sans-serif;margin:0 auto 0 auto;padding:0 15px 0 15px;table-layout:auto;text-align:left">
<tr><td bgcolor="#ffffff" height="20" style="border-collapse:collapse;font-size:0;line-height:0">&nbsp;</td></tr>
<tr>
<td align="center" style="background:#ffffff;border-collapse:collapse;color:#3d454c;font:21px / 21px 'Open Sans','helvetica' , sans-serif;height:40px;padding:0 16px 0 16px;text-align:center">
{{ title }}
</td>
</tr>
</table>
<table  width="{{ width }}" border="0" cellpadding="0" cellspacing="0" style="font-family:Open Sans,Helvetica,sans-serif;font-size:16px;line-height:24px;color:#3d454c;text-align:left;padding:0 15px 0 15px;border-spacing:0;border-collapse:collapse;table-layout:auto;margin:0 auto">
  <tr>
    <td align="left" height="40" style="color:#ffffff;padding:0 15px 0 15px;background-color:rgb(0,106,193);border-collapse:collapse">
      {{ t('report','ADMIN') }}
    </td>
    <td align="right" style="color: #ffffff;padding: 0 10px 0 10px;background-color: rgb(0,106,193);border-collapse: collapse;">
      {{ t('report','TIME') }}
    </td>
  </tr>
  {% for block in data %}
    <tr>
      <td align="left" style="padding: 0 15px 0 15px;">{{ block.name }}</td>
      <td align="right" style="padding: 0 15px 0 15px;">{{ echo_duration(block.duration) | raw }}</td>
    </tr>
  {% endfor %}
<tr><td bgcolor="#ffffff" height="30" style="font-size:0;line-height:0;border-collapse:collapse">&nbsp;</td></tr>

</table>
<table border="0" cellpadding="0" cellspacing="0" style="margin: 0 auto 0 auto;background-color:#eeeeee;min-width:100%;width:100%;">
<tr><td bgcolor="#eeeeee" height="4" style="font-size:0;line-height:0;border-collapse:collapse">&nbsp;</td></tr>
</table>

